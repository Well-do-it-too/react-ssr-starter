{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node",
			"config": { "buildCommand": "npm run build" }
		},
		{
			"src": "server.js",
			"use": "@vercel/node"
		},
		{
			"src": "dist/client/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "(.*)",
			"dest": "server.js"
		}
	]
}
