{
	"name": "tm-finish",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"clean": "shx rm -rf node_modules pnpm-lock.yaml",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"rmdist": "shx rm -rf dist",
		"certs:init": "shx mkdir -p .certs && cd .certs && mkcert create-ca && mkcert create-cert --domain react-ssr-starter.dev",
		"=== ssr": "=========== SSR =============================",
		"ssr:dev": "cross-env NODE_ENV=development node server",
		"ssr:build:client": "vite build --ssrManifest --outDir dist/client",
		"ssr:build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
		"ssr:build": "pnpm run ssr:build:client && pnpm run ssr:build:server",
		"ssr:preview": "cross-env NODE_ENV=production node server",
		"======": "===========================================",
		"stylelint": "stylelint \"src/**/*.scss\"",
		"stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
		"lint": "eslint . --ext .ts",
		"format": "prettier --write \"src/**/*.ts\"",
		"pre-commit": "npm run format && npm run lint && npm run stylelint"
	},
	"keywords": [
		"typescript",
		"node",
		"starter",
		"boilerplate",
		"eslint",
		"prettier",
		"husky"
	],
	"engines": {
		"node": ">= 20 < 21"
	},
	"author": "Vitalii Rozhkov <welldoittoo@gmail.com>",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/Well-do-it-too/react-ssr-starter.git"
	},
	"bugs": {
		"url": "https://github.com/Well-do-it-too/react-ssr-starter/issues"
	},
	"homepage": "https://github.com/Well-do-it-too/react-ssr-starter#readme",
	"dependencies": {
		"axios": "^1.6.8",
		"clsx": "^2.1.1",
		"express": "^4.19.2",
		"i18next": "^23.11.3",
		"i18next-browser-languagedetector": "^7.2.1",
		"i18next-http-backend": "^2.5.1",
		"localforage": "^1.10.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.51.3",
		"react-i18next": "^14.1.1",
		"react-query": "^3.39.3",
		"react-router-dom": "^6.23.0",
		"sort-by": "^1.2.0",
		"vite": "^5.2.10",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.21",
		"@types/node": "^20.12.7",
		"@types/react": "^18.2.79",
		"@types/react-dom": "^18.2.25",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@vitejs/plugin-react-swc": "^3.6.0",
		"compression": "^1.7.4",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.5",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.11",
		"mkcert": "^3.2.0",
		"prettier": "3.3.3",
		"sass": "^1.76.0",
		"serve": "^14.2.1",
		"shx": "^0.3.4",
		"sirv": "^2.0.4",
		"stylelint": "^16.7.0",
		"stylelint-config-standard-scss": "^13.1.0",
		"stylelint-prettier": "^5.0.2",
		"typescript": "^5.4.5",
		"vite-tsconfig-paths": "^4.3.2"
	}
}
